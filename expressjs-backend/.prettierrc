{
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "proseWrap": "preserve",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": ["*.json", "*.json5", "*.jsonc"],
        "options": { "parser": "json" }
      }
    ]
  }
  